    "from sympy import Integral , symbols, Eq, exp, sqrt, init_printing\n",
    "init_printing()\n",
    "x = symbols(\"x\")\n",
    "I1=Integral(exp(sqrt(x)), x)\n",
    "I1d= Eq(I1,I1.doit())\n",
    "I1d"
